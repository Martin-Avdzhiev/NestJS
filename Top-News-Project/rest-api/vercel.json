{
    "version": 2,
    "installCommand": "npm install",
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "env": {
        "PORT": "@PORT",
        "MONGODB_URI": "@MONGODB_URI"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts"
        }
    ]
}