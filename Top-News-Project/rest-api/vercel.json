{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node",
        "builds": [
          {
            "src": "src",
            "use": "@vercel/node",
            "build": "nest build"
          }
        ]
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
      }
    ],
    "env": {
      "PORT": "PORT",
      "MONGODB_URI": "MONGODB_URI"
    }
  }